global with sharing class InvoiceService {
	
	public static List<Invoice__c> getInvoices(Set<String> fields, Set<String> status, Id programId, Integer quantity, Integer offset, String orderBy) {   
        InvoicesSelector selector = new InvoicesSelector(fields);
        selector.setOrderBy(orderBy);
        selector.setLimit(quantity);
        selector.setOffset(offset);
        return  selector.selectByProgramAndStatus(status, programId);
    }

    public static List<Invoice__c> updateInvoiceStatus(Set<Id> invoiceIds, String status){
        if(!invoiceIds.isEmpty()){
            SObjectUnitOfWork uow = new SObjectUnitOfWork(new List<Schema.SObjectType>{Invoice__c.SObjectType});
            for(Id invoiceId : invoiceIds){
                uow.registerDirty(new Invoice__c(Id = invoiceId, Status__c = status));
            }
            uow.commitWork();
            return (List<Invoice__c>)uow.m_dirtyMapByType.get('FieloPRP__Invoice__c').values();
        }
        return null;
    }

    global static Invoice__c submitInvoice(Invoice__c invoice, List<InvoiceItem__c> invoiceItems, List<Attachment> attachments){
        SObjectUnitOfWork uow = new SObjectUnitOfWork(new List<Schema.SobjectType>{Invoice__c.SObjectType, InvoiceItem__c.SObjectType, Attachment.SObjectType});
        
        if(invoice.Status__c != 'Open' && invoice.Status__c != 'New'){
            invoice.Status__c = 'Open';
        }

        if(invoice.Id == null){
            uow.registerNew(invoice);
            if (invoiceItems != null) {
                for(InvoiceItem__c item : invoiceItems){
                    uow.registerNew(item, InvoiceItem__c.Invoice__c, invoice);
                }
            }

            if(attachments != null){
                for(Attachment attachment : attachments){
                    //uow.registerNew(attachment, Attachment.ParentId, invoiceWrapper.invoice);
                }
            }
        }else{
            uow.registerDirty(invoice);
            Set<Id> existingItems = new Set<Id>();
            if(invoiceItems != null){
                for(InvoiceItem__c item : invoiceItems){
                    if(item.Id == null){
                        uow.registerNew(item, InvoiceItem__c.Invoice__c, invoice);
                    }else{
                        uow.registerDirty(item);
                        existingItems.add(item.Id);
                    }
                }
            }
            
            uow.registerDeleted(new InvoiceItemsSelector().selectByInvoiceId(new Set<Id>{invoice.Id}, existingItems));
        }

        Savepoint sp = Database.setSavepoint();
        try {
           uow.commitWork();
           return invoice;
        }catch(DMLException e){
            Database.rollback(sp);       
            throw e;
        }
    }

    global static Invoice__c closeInvoice(Id invoiceId){
        return closeInvoices(new Set<Id>{invoiceId})[0];
    }

    public static List<Invoice__c> closeInvoices(Set<Id> invoiceIds){
        if(!invoiceIds.isEmpty()){
            List<Invoice__c> invoices = new InvoicesSelector(new Set<String>{'Member__r.FieloPLT__Program__r.RequestInvoiceAuthorization__c'}).selectById(invoiceIds);
            SObjectUnitOfWork uow = new SObjectUnitOfWork(new List<Schema.SObjectType>{Invoice__c.SObjectType});
            for(Invoice__c invoice : invoices){
                if(invoice.Member__r.FieloPLT__Program__r.RequestInvoiceAuthorization__c){
                    invoice.Status__c = 'Pending for Approval';
                    uow.registerForApproval(invoice.Id, 'Invoice subbmited for Approval');
                }else{
                    invoice.Status__c = 'Approved';
                }
                uow.registerDirty(invoice);
            }
            uow.commitWork();
            return (List<Invoice__c>)uow.m_dirtyMapByType.get('FieloPRP__Invoice__c').values();
        }
        return null;
    }

    public static void reprocessInvoice(Set<Id> invoiceIds){
        List<Invoice__c> invoices = new InvoicesSelector( Schema.getGlobalDescribe().get('FieloPRP__Invoice__c').getDescribe().fields.getMap().keySet() ).selectById(invoiceIds);
        if(!invoices.isEmpty()){
            FieloPLT.SObjectService.processRecords(invoices, null );
        }
    }

    public static Boolean isLockedByApprovalProcess(Set<Id> invoiceIds){
        List<ProcessInstance> approvalProcessInstanceList = InvoicesSelector.selectInvoiceApprovalProcessInstance(invoiceIds);

        for(ProcessInstance pi: approvalProcessInstanceList){
            if(pi.StepsAndWorkitems!=null){
                if(!pi.StepsAndWorkitems.isEmpty()){
                    return true;
                }
            }
        }

        return false;
    }

    public static void submitForApproval(Set<Id> recordIds){
        try{
            SObjectUnitOfWork uow = new SObjectUnitOfWork(new List<Schema.SObjectType>{Invoice__c.SObjectType});
            for(Id recordId : recordIds){
                uow.registerDirty(new Invoice__c(Id = recordId, Status__c = 'Pending for Approval'));
                uow.registerForApproval(recordId, 'Invoice submitted for Approval');
            }
            uow.commitWork();
        }
        catch(Exception e){ System.debug('Approval MSG: ' + e); }
    }

    public static void unlockRecord(Set<Id> recordIds, Boolean allOrNothing){
        try{
            List<Approval.UnlockResult> result = System.Approval.unlock(new List<Id>(recordIds));
        }
        catch(Exception e){System.debug('Approval msg: '+e);}
    }

