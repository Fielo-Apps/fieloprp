        selector.setOrderBy(orderBy);
        selector.setLimit(quantity);
        selector.setOffset(offset);
        return  selector.selectByProgramAndStatus(status, programId);
    }

    public static List<SObject> getInvoices(Set<String> fields, Id memberId, String whereClause, Integer quantity, Integer offset, String orderBy) {
        InvoicesSelector selector = new InvoicesSelector(fields);
        selector.setOrderBy(orderBy);
        selector.setLimit(quantity);
        selector.setOffset(offset);
        return  selector.selectByMemberIdAndWhereClause(memberId, whereClause);
    }

    public static List<Invoice__c> updateInvoiceStatus(Set<Id> invoiceIds, String status){
        if(!invoiceIds.isEmpty()){
            SObjectUnitOfWork uow = new SObjectUnitOfWork(new List<Schema.SObjectType>{Invoice__c.SObjectType});
            
            if(!Schema.sObjectType.Invoice__c.isUpdateable())
                throw new FieloPLT.FieloException('Insufficient permission to update invoices.');

            for(Id invoiceId : invoiceIds){
                uow.registerDirty(new Invoice__c(Id = invoiceId, Status__c = status));
            }
            if (Schema.sObjectType.Invoice__c.isUpdateable()) {
                uow.commitWork();
            } else {
                throw new FieloException(String.format(Label.PermissionOnObject,new List<String>{'FieloPRP__Invoice__c'}));
            }
            return (List<Invoice__c>)uow.m_dirtyMapByType.get('FieloPRP__Invoice__c').values();
        }
        return null;
    }

    global static Invoice__c submitInvoice(Invoice__c invoice, List<InvoiceItem__c> invoiceItems, List<Attachment> attachments){
        SObjectUnitOfWork uow = new SObjectUnitOfWork(new List<Schema.SobjectType>{Invoice__c.SObjectType, InvoiceItem__c.SObjectType, Attachment.SObjectType});
        
        if(invoice.Status__c != 'Open' && invoice.Status__c != 'New'){
            invoice.Status__c = 'Open';
        }

        if(invoice.Id == null){
            if(!Schema.sObjectType.Invoice__c.isCreateable())
                throw new FieloPLT.FieloException('Insufficient permission to create invoices.');

            uow.registerNew(invoice);
            if (invoiceItems != null) {
                for(InvoiceItem__c item : invoiceItems){
                    uow.registerNew(item, InvoiceItem__c.Invoice__c, invoice);
                }
            }

            if(attachments != null){
                for(Attachment attachment : attachments){
                    //uow.registerNew(attachment, Attachment.ParentId, invoiceWrapper.invoice);
                }
            }
        }else{
            if(!Schema.sObjectType.Invoice__c.isUpdateable())
                throw new FieloException(String.format(Label.PermissionOnObject,new List<String>{'FieloPRP__Invoice__c'}));

            uow.registerDirty(invoice);
            Set<Id> existingItems = new Set<Id>();
            if(invoiceItems != null){
                for(InvoiceItem__c item : invoiceItems){
                    if(item.Id == null){
                        if(!Schema.sObjectType.InvoiceItem__c.isCreateable())
                            throw new FieloException(String.format(Label.PermissionOnObject,new List<String>{'FieloPRP__InvoiceItem__c'}));

                        uow.registerNew(item, InvoiceItem__c.Invoice__c, invoice);
                    }else{
                        if(!Schema.sObjectType.InvoiceItem__c.isUpdateable())
                            throw new FieloException(String.format(Label.PermissionOnObject,new List<String>{'FieloPRP__InvoiceItem__c'}));

                        uow.registerDirty(item);
                        existingItems.add(item.Id);
                    }
                }
            }
            if(!Schema.sObjectType.InvoiceItem__c.isCreateable())
                throw new FieloException(String.format(Label.PermissionOnObject,new List<String>{'FieloPRP__InvoiceItem__c'}));

            uow.registerDeleted(new InvoiceItemsSelector().selectByInvoiceId(new Set<Id>{invoice.Id}, existingItems));
        }

        Savepoint sp = Database.setSavepoint();
        try {
           uow.commitWork();
           return invoice;
        }catch(DMLException e){
            Database.rollback(sp);       
            throw e;
        }
    }

