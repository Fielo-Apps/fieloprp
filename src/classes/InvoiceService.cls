    public static List<Invoice__c> closeInvoices(Set<Id> invoiceIds){
        if(!invoiceIds.isEmpty()){
            List<Invoice__c> invoices = new InvoicesSelector(new Set<String>{'Member__r.FieloPLT__Program__r.RequestInvoiceAuthorization__c'}).selectById(invoiceIds);
            SObjectUnitOfWork uow = new SObjectUnitOfWork(new List<Schema.SObjectType>{Invoice__c.SObjectType});
            for(Invoice__c invoice : invoices){
                if(invoice.Member__r.FieloPLT__Program__r.RequestInvoiceAuthorization__c){
                    invoice.Status__c = 'Pending for Approval';
                    uow.registerForApproval(invoice.Id, Label.FirstStepComments);
                }else{
                    invoice.Status__c = 'Approved';
                }
                
                if(!Schema.sObjectType.Invoice__c.isUpdateable())
                    throw new FieloPLT.FieloException('Insufficient permission to update invoices.');

                uow.registerDirty(invoice);
            }
            uow.commitWork();
            return (List<Invoice__c>)uow.m_dirtyMapByType.get('FieloPRP__Invoice__c').values();
        }
        return null;
    }

